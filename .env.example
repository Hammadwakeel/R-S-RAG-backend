# --- Project Settings ---
PROJECT_NAME="RAG AI Backend"
API_V1_STR="/api/v1"
ENV_MODE="development"

# --- Supabase (Database & Auth) ---
# Get these from your Supabase Project Settings -> API
SUPABASE_URL="https://your-project-id.supabase.co"
SUPABASE_KEY="your-supabase-service-role-key"
DB_CONNECTION_STRING="postgresql://postgres:password@db.your-project.supabase.co:5432/postgres"

# --- Vector Database (Qdrant) ---
QDRANT_URL="https://your-cluster.qdrant.io"
QDRANT_API_KEY="your-qdrant-api-key"

# --- LLM Inference (Groq) ---
# Used for ultra-fast generation (Llama 3, Mixtral)
GROQ_API_KEY="gsk_your_groq_api_key"

# --- Embeddings (Voyage AI) ---
# Used for high-quality document embeddings
VOYAGE_API_KEY="pa-your-voyage-api-key"

# --- Google Gemini (Optional Fallback) ---
GOOGLE_API_KEY="your-google-gemini-key"

# âœ… Correct Format (JSON Array with double quotes)
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]